{
  "name": "codenotch-admin-storybook",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cn:build": "tsc -b && vite build && node --experimental-json-modules codenotch/dsp.mjs",
    "cn:publish": "npm run cn:build && node --experimental-json-modules codenotch/publish.mjs"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.28.0",
    "@headlessui/react": "^2.2.7",
    "@tailwindcss/vite": "4.1.11",
    "azure-storage": "^2.10.7",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.12",
    "ora": "^8.2.0",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-docgen-typescript": "^2.4.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.7.1",
    "tailwindcss": "4.1.11",
    "ts-md5": "^2.0.1",
    "vite-plugin-compression2": "^2.2.0",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.19.0",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-onboarding": "^9.0.18",
    "@storybook/react-vite": "^9.0.18",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^9.0.18",
    "globals": "^15.14.0",
    "storybook": "^9.0.18",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.1"
  },
  "codenotch": {
    "library-name": "@codenotch/admin-v2",
    "company-name": "Codenotch SA",
    "company-url": "https://codenotch.io",
    "description": "Codenotch Admin library with Storybook",
    "author": "Romane Donnet <romane@codenot.ch> (Codenotch SA)",
    "logo": "https://colorful-cow-6efae0f56a.media.strapiapp.com/logo_icon_16b635acf7.svg"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
